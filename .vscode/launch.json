{
    "configurations": [
    {
        "name": "windows.gdb",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/debug/main.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": ".",
        "environment": [],

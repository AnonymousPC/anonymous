{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "debug.compile",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-Ddebug=1",
                "-std=c++26",
             // "-Wa,-mbig-obj",
                "-I/opt/homebrew/include",
                "-g",
                "-Wall",
                "-pedantic",
                "-fdiagnostics-color=always",
                "-flarge-source-files",
                "-fno-inline",
                "-c",
                "main.cpp",
                "-o",
                "bin/debug/main.o"
            ],
            "group": "build",
            "problemMatcher": "$gcc",
            "detail": "debug.compile"
        },
        {
            "label": "debug.link",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-o",
                "bin/debug/main",
                "bin/debug/main.o",
                "bin/dll/main.dylib",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/libstdc++exp.a",
                "/opt/homebrew/lib/libboost_date_time-mt.dylib",
                "/opt/homebrew/lib/libboost_filesystem-mt.dylib",
                "/opt/homebrew/lib/libboost_iostreams.dylib",
                "/opt/homebrew/lib/libboost_locale-mt.dylib",
                "/opt/homebrew/lib/libboost_process-mt.dylib",
                "/opt/homebrew/lib/libboost_stacktrace_basic-mt.dylib",
                "/opt/homebrew/lib/libboost_system-mt.dylib",
                "/opt/homebrew/lib/libssl.dylib",
                "/opt/homebrew/lib/libcrypto.dylib",
                "/opt/homebrew/lib/libjpeg.dylib",
                "/opt/homebrew/lib/libpng.dylib",
                "/opt/homebrew/lib/libtiff.dylib",
                "/opt/homebrew/lib/libmpg123.dylib",
                "/opt/homebrew/lib/libFLAC++.dylib"
            ],
            "group": "build",
            "detail": "debug.link"
        },
        {
            "label": "debug.run",
            "type": "shell",
            "command": "bin/debug/main",
            "group": "build",
            "detail": "debug.run"
        },
        {
            "label": "debug",
            "type": "shell",
            "dependsOn": ["debug.compile", "debug.link"],
            "dependsOrder": "sequence",
            "command": "bin/debug/main",
            "group": "build",
            "detail": "debug"
        },








        {
            "label": "release.compile",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-std=c++26",
                "-O3",
             // "-Wa,-mbig-obj",
                "-I/opt/homebrew/include",
                "-g",
                "-Wall",
                "-pedantic",
                "-fdiagnostics-color=always",
                "-flarge-source-files",
                "-c",
                "main.cpp",
                "-o",
                "bin/release/main.o"
            ],
            "group": "build",
            "problemMatcher": "$gcc",
            "detail": "release.compile"
        },
        {
            "label": "release.link",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-o",
                "bin/release/main",
                "bin/release/main.o",
                "bin/dll/main.dylib",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/libstdc++exp.a",
                "/opt/homebrew/lib/libboost_date_time-mt.dylib",
                "/opt/homebrew/lib/libboost_filesystem-mt.dylib",
                "/opt/homebrew/lib/libboost_iostreams.dylib",
                "/opt/homebrew/lib/libboost_locale-mt.dylib",
                "/opt/homebrew/lib/libboost_process-mt.dylib",
                "/opt/homebrew/lib/libboost_stacktrace_basic-mt.dylib",
                "/opt/homebrew/lib/libboost_system-mt.dylib",
                "/opt/homebrew/lib/libssl.dylib",
                "/opt/homebrew/lib/libcrypto.dylib",
                "/opt/homebrew/lib/libjpeg.dylib",
                "/opt/homebrew/lib/libpng.dylib",
                "/opt/homebrew/lib/libtiff.dylib",
                "/opt/homebrew/lib/libmpg123.dylib",
                "/opt/homebrew/lib/libFLAC++.dylib"
            ],
            "group": "build",
            "detail": "release.link"
        },
        {
            "label": "release.run",
            "type": "shell",
            "command": "bin/release/main",
            "group": "build",
            "detail": "release.run"
        },
        {
            "label": "release",
            "type": "shell",
            "dependsOn": ["release.compile", "release.link"],
            "dependsOrder": "sequence",
            "command": "bin/release/main",
            "group": "build",
            "detail": "release"
        },











        {
            "label": "dll.compile",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-Ddll=1",
                "-std=c++26",
                "-O3",
             // "-Wa,-mbig-obj",
                "-I/opt/homebrew/include",
                "-g",
                "-Wall",
                "-pedantic",
                "-fdiagnostics-color=always",
                "-flarge-source-files",
                "-c",
                "main.cpp",
                "-o",
                "bin/dll/main.o"
            ],
            "group": "build",
            "problemMatcher": "$gcc",
            "detail": "dll.compile"
        },
        {
            "label": "dll.link",
            "type": "process",
            "command": "g++-14",
            "args": [
                "-shared",
                "-o",
                "bin/dll/main.dylib",
                "bin/dll/main.o",
                "/opt/homebrew/Cellar/gcc/14.2.0_1/lib/gcc/current/libstdc++exp.a",
                "/opt/homebrew/lib/libboost_date_time-mt.dylib",
                "/opt/homebrew/lib/libboost_filesystem-mt.dylib",
                "/opt/homebrew/lib/libboost_iostreams.dylib",
                "/opt/homebrew/lib/libboost_locale-mt.dylib",
                "/opt/homebrew/lib/libboost_process-mt.dylib",
                "/opt/homebrew/lib/libboost_stacktrace_basic-mt.dylib",
                "/opt/homebrew/lib/libboost_system-mt.dylib",
                "/opt/homebrew/lib/libssl.dylib",
                "/opt/homebrew/lib/libcrypto.dylib",
                "/opt/homebrew/lib/libjpeg.dylib",
                "/opt/homebrew/lib/libpng.dylib",
                "/opt/homebrew/lib/libtiff.dylib",
                "/opt/homebrew/lib/libmpg123.dylib",
                "/opt/homebrew/lib/libFLAC++.dylib"
            ],
            "group": "build",
            "detail": "dll.link"
        },
        {
            "label": "dll",
            "type": "shell",
            "dependsOn": ["dll.compile", "dll.link"],
            "dependsOrder": "sequence",
            "command": "print dll_ok",
            "group": "build",
            "detail": "dll"
        },
    ]
}
